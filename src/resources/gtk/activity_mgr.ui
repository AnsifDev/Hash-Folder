<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<object class="AdwHeaderBar" id="header_bar">
		<child type="start">
			<object class="GtkButton" id="nav_back">
				<property name="icon-name">go-previous</property>
				<property name="visible">false</property>
			</object>
		</child>
		<!-- <style><class name="devel"/></style> -->
	</object>
	<object class="AdwBin" id="container">
		<child>
			<object class="GtkStack" id="stack">
				<child>
					<object class="AdwStatusPage">
						<property name="title">Screen Size Too Small</property>
						<property name="description">Sorry your device screen size is too small for this application</property>
						<property name="icon-name">error-symbolic</property>
						<style><class name="compact"/></style>
					</object>
				</child>
			</object>
		</child>
	</object>
</interface>